<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IAdministrativosService" />
                <binding name="BasicHttpBinding_IAulaService" />
                <binding name="BasicHttpBinding_ICarreraService" />
                <binding name="BasicHttpBinding_IEstudianteService" />
                <binding name="BasicHttpBinding_IGrupo_EstudianteService" />
                <binding name="BasicHttpBinding_IGrupoServicio" />
                <binding name="BasicHttpBinding_IProfesorService" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:55919/WebServices/Servicies/AdministrativosService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IAdministrativosService"
                contract="AdministrativoService.IAdministrativosService" name="BasicHttpBinding_IAdministrativosService" />
            <endpoint address="http://localhost:55919/WebServices/Servicies/AulaService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IAulaService"
                contract="AulaService.IAulaService" name="BasicHttpBinding_IAulaService" />
            <endpoint address="http://localhost:55919/WebServices/Servicies/CarreraService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICarreraService"
                contract="CarreraServicio.ICarreraService" name="BasicHttpBinding_ICarreraService" />
            <endpoint address="http://localhost:55919/WebServices/Servicies/Grupo_EstudianteService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IGrupo_EstudianteService"
                contract="Grupo_EstudianteService.IGrupo_EstudianteService"
                name="BasicHttpBinding_IGrupo_EstudianteService" />
            <endpoint address="http://localhost:55919/WebServices/Servicies/GrupoServicio.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IGrupoServicio"
                contract="GrupoService.IGrupoServicio" name="BasicHttpBinding_IGrupoServicio" />
            <endpoint address="http://localhost:55919/WebServices/Servicies/EstudianteService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IEstudianteService"
                contract="EstudianteSerivice.IEstudianteService" name="BasicHttpBinding_IEstudianteService" />
            <endpoint address="http://localhost:55919/WebServices/Servicies/ProfesorService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IProfesorService"
                contract="ProfesorService.IProfesorService" name="BasicHttpBinding_IProfesorService" />
        </client>
    </system.serviceModel>
</configuration>